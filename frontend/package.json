{
    "name": "jumppaliput",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://backend",
    "dependencies": {
        "date-fns": "^1.29.0",
        "emotion": "^9.1.1",
        "emotion-theming": "^9.0.0",
        "interpolate": "^0.1.0",
        "mobx": "^4.1.1",
        "mobx-react": "^5.0.0",
        "moment": "^2.29.4",
        "node-pre-gyp": "^0.17.0",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-emotion": "^9.1.1",
        "react-html-parser": "^2.0.2",
        "react-pose": "^1.5.2",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-scripts": "^2.1.8",
        "utils": "file:src/utils"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "export PUBLIC_URL=\"/app\" && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",
        "format": "./node_modules/.bin/prettier --write",
        "lint": "./node_modules/.bin/eslint src",
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "husky": "^0.14.3",
        "lint-staged": "^7.3.0",
        "prettier": "^1.11.1"
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "babel": {
        "plugins": [
            "transform-decorators-legacy"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
